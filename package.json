{
	"name": "helloworld-minimal-sample",
	"description": "Minimal HelloWorld example for VS Code",
	"version": "0.0.1",
	"publisher": "vscode-samples",
	"repository": "https://github.com/Microsoft/vscode-extension-samples/helloworld-minimal-sample",
	"engines": {
		"vscode": "^1.32.0"
	},
	"activationEvents": [
		"onStartupFinished"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.HELLOWORLD",
				"title": "HELLOWORLD"
			}
		],
		"keybindings": [
			{
				"command": "extension.HELLOWORLD",
				"key": "ctrl+alt+shift+F1"
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "VIEW_CONTAINER",
					"title": "HELLOWORLD",
					"icon": "media/icon.png"
				}
			]
		},
		"views": {
			"VIEW_CONTAINER": [
				{
					"id": "HELLOWORLD_VIEW",
					"name": "HELLOWORLD_VIEW"
				}
			]
		},
		"viewsWelcome": [
			{
				"view": "HELLOWORLD_VIEW",
				"contents": "HELLOWORLD\n[HELLOWORLD](command:extension.HELLOWORLD)\n"
			}
		]
	},
	"scripts": {},
	"devDependencies": {
		"@types/vscode": "^1.32.0"
	}
}